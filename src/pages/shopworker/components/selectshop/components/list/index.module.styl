.shop_items
  border-bottom: 1Px solid #F5F5F5
  height:230px
  padding-top:25px
  padding-bottom: 25px
  padding-left:220px
  position relative
  &:last-child
    border-bottom: none

.shop_img
  width:178px
  position:absolute
  left:20px
  top:25px
.saleEnd
  position absolute
  left:0
  top:0
  width:178px
  height:178px
  background:rgba(0,0,0,0.4);
  border-radius: 6px
  z-index: 8
  display: flex
  justify-content: center
  align-items: center
.saleEnd_img
  width:95px
  height:95px
  display:block
  border-radius: 6px
.image
  background-image:url('data:image/png;base64,')
  background-repeat: no-repeat
  background-position: center center
  background-color: #ebebeb
  background-size: 100% auto
  width:178px
  height:178px
  border-radius: 6px

.shop_title
  font-size: 28px
  color:#333

.price_box
  position: absolute
  bottom:20px

.now_price
  color:#F9482E
  font-size: 28px

.old_price
  color:#B3B3B3
  font-size: 24px
  text-decoration:line-through
  margin-left:30px

.handler_box
  position absolute
  right:20px
  bottom:20px

.up_shop_btn
  background-color: #F9482E
  color:white
  font-size: 24px
  text-align: center
  height:44px
  line-height: 44px
  border-radius: 24px
  padding-left:15px
  padding-right:15px