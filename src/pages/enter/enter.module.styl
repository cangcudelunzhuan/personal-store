
.container
  background #F8F8F8
  position fixed
  left 0
  width 100%
  height 100%
.store_info
  background rgba(255, 255, 255, 1)
  border-radius 0px 0px 16px 16px
  padding 56px 28px 36px
.title
  height 33px
  font-size 24px
  font-weight 400
  color rgba(179, 179, 179, 1)
  line-height 33px
.list
  position relative
  font-size 28px
  font-weight 400
  color #333
  line-height 40px
  padding 28px 0
  display flex
  align-items center
  position: relative
  &:after
    position: absolute
    content: ''
    width: 200%
    height: 1px
    background: #ddeedd
    left: -50%
    bottom: 0
    transform: scale(0.5)
.store_title
  width 150px
  line-height 40px
.symbol
  color rgba(249, 72, 46, 1)
  margin-right 10px
.store_insert
  font-size 28px
  font-weight 500
  color rgba(51, 51, 51, 1)
  line-height 40px
  margin 0 0 0 60px
input::-webkit-input-placeholder
  color #ccc
.aign
  justify-content space-between
.icon
  color #F9482E
  font-size 38px
.unagree
  font-size 38px
  color #333
  margin-right 20px
.active
  color rgba(249, 72, 46, 1)
.bug_info
  text-align center
  font-size 24px
  font-weight 400
  line-height 33px
  color #989898
  margin-top 60px
  width 100%
  padding-bottom constant(safe-area-inset-bottom)
  padding-bottom env(safe-area-inset-bottom)
.red_text
  color #F9482E
.tel
  margin 36px 0 48px 0
  font-size 24px
  font-weight 400
  color rgba(153, 153, 153, 1)
  line-height 33px
.buy_btn
  height 68px
  background rgba(249, 72, 46, 1)
  border-radius 34px
  margin 0 83px
  font-size 28px
  font-weight 400
  color rgba(255, 255, 255, 1)
  line-height 68px
  text-align center
  border: 1px solid rgba(249, 72, 46, 1)
.buy_btn_gray
  background rgba(232, 233, 235, 1)
  border-color: rgba(232, 233, 235, 1)
.white_btn
  background: #ffffff
  color: #999999
  border-color: #bebebe
  margin-bottom: 30px
.checkline
  display flex
  align-items center
  justify-content center
.nocheck
  width 10px
  height 10px
  border 2px solid #ddd
  padding 7px
  border-radius 2px
  margin-right 30px
.xiey
  text-align left
  font-size 20px
  text-indent 30px


.login_out
  color: #aaa
  font-size: 28px
  margin-top: 30px

.item_btn+.item_btn
  margin-top: 30px