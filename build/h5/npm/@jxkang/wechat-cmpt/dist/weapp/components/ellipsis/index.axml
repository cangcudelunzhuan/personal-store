<block a:if="{{$taroCompReady}}">
    <block>
        <block a:if="{{count === -1}}">
            <view class="{{className}}">
                <slot></slot>
            </view>
        </block>
        <block a:else>
            <view class="{{anonymousState__temp}}">
                <slot></slot>
            </view>
        </block>
    </block>
</block>