<block a:if="{{$taroCompReady}}">
    <view class="{{anonymousState__temp}}" style="{{anonymousState__temp2}}">
        <slot></slot>
        <block>
            <block a:if="{{dot}}">
                <view class="at-badge__dot"></view>
            </block>
            <block a:else>
                <block a:if="{{val !== ''}}">
                    <view class="at-badge__num">{{val}}</view>
                </block>
            </block>
        </block>
    </view>
</block>